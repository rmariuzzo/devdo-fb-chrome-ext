{
  "name": "Developers.do",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This extensions is intented to be used for members of http://www.facebook.com/groups/devdominicanos/",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "*://www.facebook.com/groups/devdominicanos/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/groups/devdominicanos/*"
      ],
      "css": [
        "vendors/highlight.css"
      ],
      "js": [
        "vendors/jquery.js",
        "vendors/highlight.js",
        "main.js"
      ]
    }
  ]
}